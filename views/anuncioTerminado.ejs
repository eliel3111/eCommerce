<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>E-Commerce</title>

  <!-- Preconnect for Google Fonts -->
  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>

  <!-- Google Fonts: Inter, Roboto, DM Serif, Montserrat -->
  <link href="https://fonts.googleapis.com/css2?family=Inter:wght@400;700;900&family=Roboto&family=DM+Serif+Text:ital@0;1&family=Montserrat&display=swap" rel="stylesheet">

  <!-- Your CSS -->
  <link rel="stylesheet" href="/styles/header.css">
  <link rel="stylesheet" href="/styles/anuncios.css">
</head>

<body >
  <%- include("partials/header.ejs")%>

<div class="anuncioPage">
  <div class="sidebar"></div>
  <div class="mainAnuncio">
    <!--Area de titulo y botones de guardar-->
    <div class="tituloContenedor boxAnuncio">
        <!--Titulo del Anuncio-->
        <div class="tituloAnuncio">
            <!--Texto del Titulo-->
            <div class="titulo-anuncio-text"><%= anuncio.titulo %></div>
            <!--Icono Atras para movil pantalla-->
            <div class="icono-atras-movil">
                <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" fill="currentColor" viewBox="0 0 16 16">
                    <path fill-rule="evenodd" d="M15 8a.5.5 0 0 1-.5.5H2.707l4.147 4.146a.5.5 0 0 1-.708.708l-5-5a.5.5 0 0 1 0-.708l5-5a.5.5 0 1 1 .708.708L2.707 7.5H14.5A.5.5 0 0 1 15 8z"/>
                </svg>
            </div>
        </div>
        <!--Boton de Guardar-->
        <div class="guardarAnuncio-container">
            <div id="save-ad-btn" data-anuncio-id="<%= anuncio.id %>" class="guardar-btn">
                <!-- Ícono corazón -->
                <svg xmlns="http://www.w3.org/2000/svg" fill="black" class="icono" viewBox="0 0 24 24">
                    <path d="M12 21.35l-1.45-1.32C5.4 15.36 2 12.28 2 8.5 
                            2 5.42 4.42 3 7.5 3c1.74 0 3.41 0.81 4.5 2.09
                            C13.09 3.81 14.76 3 16.5 3
                            19.58 3 22 5.42 22 8.5
                            c0 3.78-3.4 6.86-8.55 11.54L12 21.35z"/>
                </svg>

                <!-- Texto -->
                <span class="texto">Guardar</span>
            </div>    
        </div>
    </div>
    <div class="boxAnuncio imagenes-contenedor">
        <div class="carousel" id="carousel"></div>

        <% if (anuncio.imagenes.length >= 5) { %>
            <button class="prev-img-button" id="prev">
                <svg width="24" height="24" viewBox="0 0 24 24">
                    <path d="M15 18l-6-6 6-6" stroke="#333" stroke-width="2" fill="none" stroke-linecap="round" stroke-linejoin="round"/>
                </svg>
            </button>
            <button class="next-img-button"  id="next">
                <svg width="24" height="24" viewBox="0 0 24 24">
                <path d="M9 6l6 6-6 6" stroke="#333" stroke-width="2" fill="none" stroke-linecap="round" stroke-linejoin="round"/>
                </svg>
            </button>
            <% } %>

            <div class="image-counter-box">
                <span class="image-counter-current">5</span>
                <span class="image-counter-separator">/</span>
                <span class="image-counter-total">10</span>
            </div>




            <!-- Modal -->
            <!-- Modal -->
<div id="modal" class="modalImagen">
  <div class="modal-header">
    <button id="closeModal" class="close-btn">✕ Close</button>
    <div class="contador" id="contador">2 / 30</div>
  </div>

  <div class="modal-contenido">
    <button class="nav-btn left" id="prevBtn">❮</button>
    <img id="modalImage" src="https://via.placeholder.com/900x600" alt="Imagen del anuncio">
    <button class="nav-btn right" id="nextBtn">❯</button>
  </div>
</div>



    </div>
    <!--Slider para imagenes movil-->
    <div class="boxAnuncio carousel-container">
        <div class="carousel-images">
                <% anuncio.imagenes.forEach((imgObj, index) => { %>
                    <div class="slide slideMovil">
                    <img src="<%= imgObj.image_url %>" alt="Imagen <%= index + 1 %>" />
                    </div>
                <% }) %>
        </div>
        <button class="carousel-btn prev-btn" >
                <svg width="24" height="24" viewBox="0 0 24 24">
                    <path d="M15 18l-6-6 6-6" stroke="white" stroke-width="2" fill="none" stroke-linecap="round" stroke-linejoin="round"/>
                </svg>
            </button>
            <button class="carousel-btn next-btn">
                <svg width="24" height="24" viewBox="0 0 24 24">
                <path d="M9 6l6 6-6 6" stroke="white" stroke-width="2" fill="none" stroke-linecap="round" stroke-linejoin="round"/>
                </svg>
            </button>
    </div>
    <div  class="boxAnuncio infoAnuncio">

        <div id="sectionA">
            <div class="inner-boxB">
            <!--Texto del Titulo-->
            <div class="titulo-anuncio-movil"><%= anuncio.titulo %></div>
            
            
            <div class="precio-container">
                <div class="precio-moneda"><%= anuncio.moneda %></div>
                <div class="precio-cantidad"><%= anuncio.precio.toLocaleString() %></div>
            </div>
            <!--Area de direccion titulo-->
            <div class="anuncio-ubicacion">
                <!-- SVG de ubicación -->
                <svg class="ubicacion-icon" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor" width="20" height="20">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 11c1.657 0 3-1.343 3-3S13.657 5 12 5 9 6.343 9 8s1.343 3 3 3z" />
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 2C8.134 2 5 5.134 5 9c0 5.25 7 13 7 13s7-7.75 7-13c0-3.866-3.134-7-7-7z" />
                </svg>

                <!-- Texto de sector y ciudad -->
                <div class="ubicacion-texto"><%= anuncio.sector %>, <%= anuncio.ciudad %></div>
            </div>

            <!--Texto de cuando fue publicado-->
            <div class="anuncio-fecha">
                Publicado: 
                <%= (() => {
                    const fechaCreacion = new Date(anuncio.creado_en);
                    const fechaHoy = new Date();
                    const diferenciaMS = fechaHoy - fechaCreacion; // diferencia en milisegundos
                    const dias = Math.floor(diferenciaMS / (1000 * 60 * 60 * 24)); // convertir a días
                    return dias === 0 ? "hoy" : `hace ${dias} día${dias > 1 ? "s" : ""}`;
                })() %>
            </div>
            </div> <!--inner-boxB-->
        </div><!--sectionA-->


        <!--INFORMACION DE CONTACTO-->
        <div id="floatingBox">
          <div class="info-box">  
            <!-- Etiqueta de publicación -->
            <p class="info-label">Publicado por:</p>

            <!-- Fila de usuario -->
            <div class="user-row">
                <img  
                class="user-avatar"
                >
                <span class="user-name"><%= anuncio.usuario.nombre %></span>
            </div>

            <!-- Teléfono -->
            <p class="phone-label">Teléfonos:</p>

            <!-- Botón de contacto -->
            <button id="btnContacto" class="action-btn">Contacto</button>
          </div>  
        </div>
    </div>



